{
  "expo": {
    "name": "やりたいこと発掘アプリ",
    "description": "自分のやりたいことを発掘することができるアプリ",
    "slug": "DreamDigger",
    "version": "1.2.0",
    "orientation": "portrait",
    "icon": "./assets/images/app-icon.png",
    "scheme": "DreamDigger",
    "userInterfaceStyle": "automatic",
    "platforms": [
      "ios"
    ],
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "DreamDigger"
    },
    "notification": {
      "icon": "./assets/images/app-icon.png"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "ea69567c-2c1b-4f0e-92fb-4960040d6611"
      }
    }
  }
}
